<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
		<TargetFramework>netstandard2.1</TargetFramework>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
		<Version>2.0.0</Version>
		<Description>Provides Font Awesome icons in MVC Razor views that can be written using standard C# code.</Description>
		<PackageProjectUrl>https://github.com/porrey/Razor-Tools</PackageProjectUrl>
		<RepositoryUrl>https://github.com/porrey/Razor-Tools.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<NeutralLanguage>en-US</NeutralLanguage>
		<Copyright>Copyright © Daniel Porrey 2014-2020</Copyright>
		<Authors>Daniel M Porrey</Authors>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<PackageReleaseNotes>Updated for Microsoft AspNetCore.</PackageReleaseNotes>
		<PackageIcon>Mvc.RazorTools.Charts.png</PackageIcon>
		<AssemblyName>Mvc.RazorTools.FontAwesome</AssemblyName>
		<RootNamespace>Mvc.RazorTools.FontAwesome</RootNamespace>  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Mvc.RazorTools.Base\Mvc.RazorTools.Base.csproj" />
  </ItemGroup>

</Project>
